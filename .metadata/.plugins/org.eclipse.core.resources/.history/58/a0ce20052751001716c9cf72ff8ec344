package DataStrom;

/**
 * @author jinyu
 *
 */
public class ServerModel {
/**
 * 服务IP
 */
public String IP;
/**
 * 服务端口
 */
public int port;

/**
 * 服务名称
 */
public String name;
/*
 * 通讯方式
 */
public byte netType;

/*
 * 是否是主从服务
 */
public boolean master_slave;

/*
 * 是否是主服务
 */
public boolean isMaster;

}
